<UserControl x:Class="REITs.REITDisplayModule.Views.REITView"
			 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
			 xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
			 xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity"
			 xmlns:prism="http://prismlibrary.com/"
			 xmlns:local="clr-namespace:REITs.REITDisplayModule.Views"
			 xmlns:inf="clr-namespace:REITs.REITDisplayModule"
			 xmlns:vm="clr-namespace:REITs.REITDisplayModule.ViewModels"
			 xmlns:CustomControls="clr-namespace:REITs.Infrastructure.CustomControls;assembly=REITs.Infrastructure"
			 xmlns:conv="clr-namespace:REITs.Infrastructure.Convertors;assembly=REITs.Infrastructure"
			 prism:ViewModelLocator.AutoWireViewModel="True"
			 mc:Ignorable="d"
			 Language="en-GB"
			 d:DesignHeight="1080"
			 d:DesignWidth="1100"
			 x:Name="REITWindow">
	<UserControl.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceDictionary Source="/REITs.Infrastructure;component/Resources/ResourceDictionary.xaml" />
			</ResourceDictionary.MergedDictionaries>

			<conv:EntityNameIdConvertor x:Key="entityNameIdConvertor" />

			<conv:StarWidthConverter x:Key="starWidthConverter" />

			<Style TargetType="TextBox">
				<Setter Property="IsReadOnly"
						Value="True" />
			</Style>

			<Style TargetType="TextBlock">
				<Setter Property="IsEnabled"
						Value="False" />
			</Style>

			<Style x:Key="TextBoxStylePassFail"
				   TargetType="TextBox">
				<Setter Property="IsReadOnly"
						Value="True" />

				<Style.Triggers>
					<Trigger Property="Text"
							 Value="Pass">
						<Setter Property="TextElement.Foreground"
								Value="Green" />
					</Trigger>

					<Trigger Property="Text"
							 Value="Fail">
						<Setter Property="TextElement.Foreground"
								Value="Red" />
					</Trigger>
				</Style.Triggers>
			</Style>

			<Style x:Key="GridViewColumnHeaderStretch"
				   TargetType="{x:Type GridViewColumnHeader}">
				<Setter Property="Width">
					<Setter.Value>
						<MultiBinding Converter="{StaticResource starWidthConverter}"
									  Mode="OneWay">
							<Binding RelativeSource="{RelativeSource FindAncestor, AncestorType={x:Type TabControl}}"
									 Path="ActualWidth" />
							<Binding RelativeSource="{RelativeSource FindAncestor, AncestorType={x:Type ListView}}" />
						</MultiBinding>
					</Setter.Value>
				</Setter>
				<!-- Same for Foreground -->
			</Style>
		</ResourceDictionary>
	</UserControl.Resources>

	<Grid Margin="10">
		<Grid>
			<Grid.RowDefinitions>
				<RowDefinition Height="5" />
				<RowDefinition Height="150" />
				<RowDefinition Height="5" />
				<RowDefinition Height="*" />
			</Grid.RowDefinitions>
			<Grid.ColumnDefinitions>
				<ColumnDefinition Width="680" />
				<ColumnDefinition Width="10" />
				<ColumnDefinition Width="*" />
			</Grid.ColumnDefinitions>

			<Grid Grid.Column="0"
				  Grid.Row="1">
				<Grid.RowDefinitions>
					<RowDefinition />
					<RowDefinition />
				</Grid.RowDefinitions>
				<Border BorderThickness="1"
						BorderBrush="DarkGreen"
						CornerRadius="10"
						Background="#FFC5DEC5"
						HorizontalAlignment="Stretch"
						Grid.RowSpan="2">

					<Grid>
						<Grid.RowDefinitions>
							<RowDefinition Height="5" />
							<RowDefinition />
							<RowDefinition Height="10" />
							<RowDefinition />
							<RowDefinition Height="15" />
						</Grid.RowDefinitions>

						<Grid.ColumnDefinitions>
							<ColumnDefinition Width="10" />
							<ColumnDefinition Width="120" />
							<ColumnDefinition Width="10" />
							<ColumnDefinition Width="120" />
							<ColumnDefinition Width="20" />
							<ColumnDefinition Width="120" />
							<ColumnDefinition Width="10" />
							<ColumnDefinition Width="120" />
							<ColumnDefinition Width="10" />
							<ColumnDefinition Width="120" />
							<ColumnDefinition Width="10" />
							<ColumnDefinition Width="120" />
							<ColumnDefinition Width="10" />
							<ColumnDefinition Width="120" />
							<ColumnDefinition Width="10" />
							<ColumnDefinition Width="120" />
							<ColumnDefinition Width="10" />
							<ColumnDefinition Width="120" />
						</Grid.ColumnDefinitions>

						<Grid Grid.Column="1"
							  Grid.ColumnSpan="5"
							  Grid.Row="1"
							  Margin="0">
							<Grid.RowDefinitions>
								<RowDefinition />
								<RowDefinition />
							</Grid.RowDefinitions>
							<TextBlock Grid.Row="0"
									   TextWrapping="Wrap"
									   Text="REIT Name"
									   FontWeight="Bold"
									   FontSize="14"
									   HorizontalAlignment="Left"
									   VerticalAlignment="Top" />
							<TextBox Grid.Row="1"
									 FontSize="12"
									 Text="{Binding REITName, UpdateSourceTrigger=PropertyChanged}"
									 VerticalContentAlignment="Center"
									 HorizontalAlignment="Stretch"
									 VerticalAlignment="Top"
									 TabIndex="4"
									 Height="25" />
						</Grid>

						<Grid Grid.Column="7"
							  Grid.Row="1"
							  Grid.ColumnSpan="2">
							<Grid.RowDefinitions>
								<RowDefinition />
								<RowDefinition />
							</Grid.RowDefinitions>
							<TextBlock Grid.Row="0"
									   TextWrapping="Wrap"
									   Text="Principal UTR"
									   FontWeight="Bold"
									   FontSize="14"
									   HorizontalAlignment="Stretch"
									   VerticalAlignment="Top" />
							<TextBox Grid.Row="1"
									 VerticalContentAlignment="Center"
									 HorizontalAlignment="Stretch"
									 VerticalAlignment="Top"
									 FontSize="12"
									 Text="{Binding PrincipalUTR, UpdateSourceTrigger=PropertyChanged}"
									 TabIndex="4"
									 MaxLength="100"
									 Height="25" />
						</Grid>

						<Grid Grid.Column="1"
							  Grid.Row="3"
							  Grid.ColumnSpan="1">
							<Grid.RowDefinitions>
								<RowDefinition />
								<RowDefinition />
							</Grid.RowDefinitions>
							<TextBlock Grid.Row="0"
									   TextWrapping="Wrap"
									   Text="APE"
									   FontWeight="Bold"
									   FontSize="14"
									   HorizontalAlignment="Stretch"
									   VerticalAlignment="Top" />
							<TextBox Grid.Row="1"
									 Width="100"
									 VerticalContentAlignment="Center"
									 HorizontalContentAlignment="Center"
									 HorizontalAlignment="Left"
									 VerticalAlignment="Top"
									 FontSize="12"
									 Text="{Binding APE, StringFormat=D, UpdateSourceTrigger=PropertyChanged}"
									 TabIndex="4"
									 MaxLength="100"
									 Height="25" />
						</Grid>

						<Grid Grid.Column="3"
							  Grid.Row="3">
							<Grid.RowDefinitions>
								<RowDefinition />
								<RowDefinition />
							</Grid.RowDefinitions>
							<TextBlock Grid.Row="0"
									   Grid.ColumnSpan="2"
									   TextWrapping="Wrap"
									   Text="Previous APE"
									   FontWeight="Bold"
									   FontSize="14"
									   HorizontalAlignment="Stretch"
									   VerticalAlignment="Top" />
							<TextBox Grid.Row="1"
									 Width="100"
									 HorizontalContentAlignment="Center"
									 VerticalContentAlignment="Center"
									 HorizontalAlignment="Left"
									 VerticalAlignment="Top"
									 FontSize="12"
									 Text="{Binding PAPE, StringFormat=D, UpdateSourceTrigger=PropertyChanged}"
									 TabIndex="4"
									 MaxLength="100"
									 Height="24" />
						</Grid>

						<Grid Grid.Column="5"
							  Grid.Row="3"
							  Grid.ColumnSpan="1">
							<Grid.RowDefinitions>
								<RowDefinition />
								<RowDefinition />
							</Grid.RowDefinitions>
							<TextBlock Grid.Row="0"
									   TextWrapping="Wrap"
									   Text="Version"
									   FontWeight="Bold"
									   FontSize="14"
									   HorizontalAlignment="Stretch"
									   VerticalAlignment="Top" />
							<TextBox Grid.Row="1"
									 VerticalContentAlignment="Center"
									 HorizontalContentAlignment="Center"
									 HorizontalAlignment="Left"
									 VerticalAlignment="Top"
									 FontSize="12"
									 Text="{Binding Version, UpdateSourceTrigger=PropertyChanged}"
									 TabIndex="4"
									 MaxLength="100"
									 Height="25"
									 Width="60" />
						</Grid>

						<Grid Grid.Column="7"
							  Grid.Row="3"
							  Grid.ColumnSpan="2"
							  Margin="-30,0,0,0">
							<Grid.RowDefinitions>
								<RowDefinition />
								<RowDefinition />
							</Grid.RowDefinitions>
							<Grid.ColumnDefinitions>
								<ColumnDefinition Width="Auto" />
								<ColumnDefinition Width="25" />
							</Grid.ColumnDefinitions>
							<TextBlock Grid.Row="0"
									   TextWrapping="Wrap"
									   Text="Date Submitted"
									   FontWeight="Bold"
									   FontSize="14"
									   HorizontalAlignment="Stretch"
									   VerticalAlignment="Top" />

							<TextBox Grid.Row="1"
									 VerticalContentAlignment="Center"
									 HorizontalContentAlignment="Center"
									 HorizontalAlignment="Stretch"
									 VerticalAlignment="Top"
									 FontSize="12"
									 Text="{Binding DateSubmitted, UpdateSourceTrigger=PropertyChanged}"
									 TabIndex="4"
									 Height="25"
									 IsReadOnly="{Binding DateSubmittedIsReadOnly}"
									 x:Name="dateSubmittedTextBox">
								<TextBox.Style>
									<Style>
										<Style.Triggers>
											<DataTrigger Binding="{Binding DateSubmittedIsReadOnly}"
														 Value="false">
												<Setter Property="TextBox.Background"
														Value="LightCoral" />
												<Setter Property="TextBox.FontWeight"
														Value="Bold" />
											</DataTrigger>
										</Style.Triggers>
									</Style>
								</TextBox.Style>
							</TextBox>

							<Button Grid.Column="1"
									Grid.Row="1"
									Height="25"
									Width="25"
									Background="Transparent"
									BorderThickness="0"
									HorizontalAlignment="Right"
									Command="{Binding ToggleSubmissionDateEdit}"
									IsHitTestVisible="{Binding ToggleSubmissionDateEditIsEnabled}">
								<Image Source="/REITs.REITDisplayModule;component/Resources/Data-Edit.png" />
							</Button>
						</Grid>

						<Grid Grid.Column="7"
							  Grid.Row="5"
							  Grid.ColumnSpan="1"
							  Margin="-30,0,0,0">
							<TextBlock Grid.Row="4"
									   IsHitTestVisible="False"
									   Text="{Binding OriginalDateSubmitted}"
									   VerticalAlignment="Top"
									   HorizontalAlignment="Center"
									   Foreground="DodgerBlue"
									   FontSize="12">
								<TextBlock.Style>
									<Style TargetType="{x:Type TextBlock}">
										<Setter Property="Visibility"
												Value="Collapsed" />
										<Style.Triggers>
											<DataTrigger Binding="{Binding DateSubmittedIsReadOnly}"
														 Value="false">
												<Setter Property="Visibility"
														Value="Visible" />
											</DataTrigger>
										</Style.Triggers>
									</Style>
								</TextBlock.Style>
							</TextBlock>
						</Grid>
						<TextBlock Grid.Column="7"
								   Grid.Row="5"
								   Grid.ColumnSpan="3"
								   Name="UpdatedSubmissionDateResultSaver"
								   FontSize="8"
								   HorizontalAlignment="Left"
								   VerticalAlignment="Top"
								   Background="Transparent"
								   Foreground="Blue"
								   Text="XML Date Changed - AutoSaving..."
								   Visibility="{Binding SDUVisibility}"
								   Margin="-30,0,0,0">
						</TextBlock>

						<Grid Grid.Column="9"
							  Grid.Row="1"
							  Grid.ColumnSpan="2"
							  Margin="20,0,0,0">
							<Grid.RowDefinitions>
								<RowDefinition />
								<RowDefinition />
							</Grid.RowDefinitions>
							<TextBlock Grid.Row="0"
									   TextWrapping="Wrap"
									   Text="Created by"
									   FontWeight="Bold"
									   FontSize="14"
									   HorizontalAlignment="Left"
									   VerticalAlignment="Top" />
							<TextBox Grid.Row="1"
									 VerticalContentAlignment="Center"
									 FontSize="12"
									 Text="{Binding CreatedBy, UpdateSourceTrigger=PropertyChanged}"
									 HorizontalAlignment="Stretch"
									 VerticalAlignment="Top"
									 TabIndex="4"
									 MaxLength="100"
									 Height="24" />
						</Grid>

						<Grid Grid.Column="9"
							  Grid.Row="3"
							  Margin="20,0,0,0">
							<Grid.RowDefinitions>
								<RowDefinition />
								<RowDefinition />
							</Grid.RowDefinitions>
							<TextBlock Grid.Row="0"
									   TextWrapping="Wrap"
									   Text="Date Created"
									   FontWeight="Bold"
									   FontSize="14"
									   HorizontalAlignment="Stretch"
									   VerticalAlignment="Top" />
							<TextBox Grid.Row="1"
									 HorizontalContentAlignment="Center"
									 VerticalContentAlignment="Center"
									 HorizontalAlignment="Left"
									 VerticalAlignment="Top"
									 FontSize="12"
									 Text="{Binding DateCreated, StringFormat=D, UpdateSourceTrigger=PropertyChanged}"
									 TabIndex="4"
									 MaxLength="100"
									 Width="100"
									 Height="24" />
						</Grid>
					</Grid>
				</Border>
			</Grid>

			<Border Grid.Column="7"
					Grid.Row="1"
					Grid.ColumnSpan="2"
					BorderThickness="1"
					BorderBrush="DarkGreen"
					CornerRadius="10"
					Background="#FFC5DEC5"
					HorizontalAlignment="Stretch">
				<Grid>
					<Grid.RowDefinitions>
						<RowDefinition Height="30" />
						<RowDefinition />
					</Grid.RowDefinitions>
					<TextBlock Grid.Row="0"
							   Margin="10,0,0,5"
							   TextWrapping="Wrap"
							   Text="Notes"
							   FontWeight="Bold"
							   FontSize="14"
							   HorizontalAlignment="Left"
							   VerticalAlignment="Bottom" />
					<TextBox Grid.Row="1"
							 Grid.RowSpan="3"
							 Grid.ColumnSpan="5"
							 Text="{Binding Notes, UpdateSourceTrigger=PropertyChanged}"
							 HorizontalAlignment="Stretch"
							 VerticalAlignment="Stretch"
							 FontSize="12"
							 TabIndex="6"
							 TextWrapping="Wrap"
							 VerticalScrollBarVisibility="Auto"
							 HorizontalContentAlignment="Stretch"
							 Margin="10,0,10,10" />
				</Grid>
			</Border>

			<Grid Grid.ColumnSpan="3"
				  Grid.Row="7">

				<ScrollViewer VerticalScrollBarVisibility="Auto">
					<TabControl SelectedItem="{Binding SelectedTab}"
								VerticalAlignment="Stretch"
								HorizontalAlignment="Stretch"
								Background="#FFC5DEC5"
								x:Name="tabController">
						<!--Tests-->
						<TabItem Header="Tests"
								 Style="{StaticResource TabItem}"
								 FontSize="18"
								 BorderBrush="Navy"
								 FontWeight="Bold"
								 Width="130"
								 IsSelected="True">
							<Grid VerticalAlignment="Stretch">
								<Grid.ColumnDefinitions>
									<ColumnDefinition Width="Auto" />
									<ColumnDefinition Width="0" />
									<ColumnDefinition Width="0" />
									<ColumnDefinition Width="0" />
									<ColumnDefinition Width="0" />
									<ColumnDefinition Width="0" />
									<ColumnDefinition Width="0" />
									<ColumnDefinition Width="*" />
									<ColumnDefinition Width="110" />
									<ColumnDefinition Width="110" />
									<ColumnDefinition Width="110" />
									<ColumnDefinition Width="20" />
								</Grid.ColumnDefinitions>
								<Grid.RowDefinitions>
									<RowDefinition Height="0" />
									<RowDefinition Height="30" />
									<RowDefinition Height="30" />
									<RowDefinition Height="30" />
									<RowDefinition Height="30" />
									<RowDefinition Height="30" />
									<RowDefinition Height="30" />
									<RowDefinition Height="30" />
									<RowDefinition Height="30" />
									<RowDefinition Height="30" />
									<RowDefinition Height="30" />
									<RowDefinition Height="30" />
									<RowDefinition Height="30" />
									<RowDefinition Height="30" />
									<RowDefinition Height="30" />
									<RowDefinition Height="30" />
									<RowDefinition Height="30" />
									<RowDefinition Height="30" />
									<RowDefinition Height="30" />
									<RowDefinition Height="30" />
								</Grid.RowDefinitions>

								<TextBlock Margin="0"
										   Grid.Column="8"
										   Grid.Row="1"
										   FontSize="14"
										   HorizontalAlignment="Center"
										   VerticalAlignment="Center"
										   Text="Percentage(%)" />
								<TextBlock Margin="0"
										   Grid.Column="9"
										   Grid.Row="1"
										   FontSize="14"
										   HorizontalAlignment="Center"
										   VerticalAlignment="Center"
										   Text="Amount(£)" />
								<TextBlock Margin="0"
										   Grid.Column="10"
										   Grid.Row="1"
										   FontSize="14"
										   HorizontalAlignment="Center"
										   VerticalAlignment="Center"
										   Text="Result" />
								<Grid Grid.Column="0"
									  Grid.Row="2"
									  Grid.ColumnSpan="12"
									  Grid.RowSpan="4">
									<Border BorderThickness="1"
											BorderBrush="DarkGreen"
											CornerRadius="10"
											Background="#FFD7F9D7"
											Margin="10,0,10,0"></Border>
								</Grid>
								<TextBlock Margin="20,5,0,4"
										   Grid.Column="0"
										   Grid.Row="2"
										   Grid.ColumnSpan="3"
										   FontSize="14"
										   HorizontalAlignment="Left"
										   Foreground="DarkBlue"
										   VerticalAlignment="Center"
										   Text="Balance of Business Tests" />
								<TextBox Margin="0,3,0,3"
										 VerticalContentAlignment="Center"
										 HorizontalContentAlignment="Right"
										 Grid.Column="8"
										 Grid.Row="3"
										 FontSize="12"
										 Text="{Binding IncomeTestPercentage, StringFormat=0.00, UpdateSourceTrigger=PropertyChanged}"
										 HorizontalAlignment="Stretch"
										 TabIndex="4"
										 MaxLength="100" />
								<TextBox Margin="0,3,0,3"
										 VerticalContentAlignment="Center"
										 HorizontalContentAlignment="Right"
										 Grid.Column="8"
										 Grid.Row="4"
										 FontSize="12"
										 Text="{Binding AssetTestPercentage, StringFormat=0.00, UpdateSourceTrigger=PropertyChanged}"
										 HorizontalAlignment="Stretch"
										 TabIndex="4"
										 MaxLength="100" />
								<TextBox Margin="0,3,0,3"
										 VerticalContentAlignment="Center"
										 HorizontalContentAlignment="Center"
										 Grid.Column="10"
										 Grid.Row="3"
										 FontSize="12"
										 Text="{Binding IncomeTestResult, UpdateSourceTrigger=PropertyChanged}"
										 HorizontalAlignment="Stretch"
										 TabIndex="4"
										 MaxLength="100"
										 Style="{StaticResource TextBoxStylePassFail}" />
								<TextBox Margin="0,3,0,3"
										 VerticalContentAlignment="Center"
										 HorizontalContentAlignment="Center"
										 Grid.Column="10"
										 Grid.Row="4"
										 FontSize="12"
										 Text="{Binding AssetTestResult, UpdateSourceTrigger=PropertyChanged}"
										 HorizontalAlignment="Stretch"
										 TabIndex="4"
										 MaxLength="100"
										 Style="{StaticResource TextBoxStylePassFail}" />

								<Grid Grid.Column="0"
									  Grid.Row="7"
									  Grid.ColumnSpan="12"
									  Grid.RowSpan="3">
									<Border BorderThickness="1"
											BorderBrush="DarkGreen"
											CornerRadius="10"
											Background="#FFD7F9D7"
											Margin="10,0,10,0"></Border>
								</Grid>
								<Grid Grid.Column="0"
									  Grid.Row="11"
									  Grid.ColumnSpan="12"
									  Grid.RowSpan="4">
									<Border BorderThickness="1"
											BorderBrush="DarkGreen"
											CornerRadius="10"
											Background="#FFD7F9D7"
											Margin="10,0,10,0"></Border>
								</Grid>

								<Grid Grid.Column="0"
									  Grid.Row="16"
									  Grid.ColumnSpan="12"
									  Grid.RowSpan="3">
									<Border BorderThickness="1"
											BorderBrush="DarkGreen"
											CornerRadius="10"
											Background="#FFD7F9D7"
											Margin="10,0,10,0"></Border>
								</Grid>

								<TextBlock Margin="20,5,0,4"
										   Grid.Column="0"
										   Grid.Row="7"
										   Grid.ColumnSpan="3"
										   FontSize="14"
										   HorizontalAlignment="left"
										   Foreground="DarkBlue"
										   VerticalAlignment="Center"
										   Text="Interest Cover Test Ratio" />
								<TextBox Margin="0,3,0,3"
										 VerticalContentAlignment="Center"
										 HorizontalContentAlignment="Right"
										 Grid.Column="8"
										 Grid.Row="8"
										 FontSize="12"
										 Text="{Binding InterestCoverRatio, StringFormat=0.00, UpdateSourceTrigger=PropertyChanged}"
										 HorizontalAlignment="Stretch"
										 TabIndex="4"
										 MaxLength="100" />
								<TextBlock FontWeight="Normal"
										   Grid.ColumnSpan="2"
										   Margin="20,5,0,4"
										   Grid.Column="0"
										   Grid.Row="8"
										   FontSize="14"
										   HorizontalAlignment="left"
										   VerticalAlignment="Center"
										   Text="Interest Cover" />
								<TextBox Name="TestresultTextBox"
										 VerticalContentAlignment="Center"
										 Margin="0,3,0,3"
										 HorizontalContentAlignment="Center"
										 Grid.Column="10"
										 Grid.Row="8"
										 FontSize="12"
										 Text="{Binding InterestCoverResult, UpdateSourceTrigger=PropertyChanged}"
										 HorizontalAlignment="Stretch"
										 TabIndex="4"
										 MaxLength="100"
										 Style="{StaticResource TextBoxStylePassFail}" />

								<TextBlock FontWeight="Normal"
										   Margin="20,5,0,4"
										   Grid.Column="0"
										   Grid.Row="3"
										   FontSize="14"
										   HorizontalAlignment="left"
										   VerticalAlignment="Center"
										   Text="Income Test" />
								<TextBlock FontWeight="Normal"
										   Margin="20,5,0,4"
										   Grid.Column="0"
										   Grid.Row="4"
										   FontSize="14"
										   HorizontalAlignment="left"
										   VerticalAlignment="Center"
										   Text="Asset Test" />

								<TextBlock Margin="20,5,0,4"
										   Grid.Column="0"
										   Grid.Row="16"
										   Grid.ColumnSpan="3"
										   FontSize="14"
										   HorizontalAlignment="left"
										   Foreground="DarkBlue"
										   VerticalAlignment="Center"
										   Text="Paid Dividends Schedule" />

								<TextBlock FontWeight="Normal"
										   Margin="20,5,0,4"
										   Grid.Column="0"
										   Grid.Row="17"
										   Grid.ColumnSpan="5"
										   FontSize="14"
										   HorizontalAlignment="left"
										   VerticalAlignment="Center"
										   Text="PID requirement met (yes/no)" />

								<ComboBox x:Name="comboBoxPaidDividendScheduleConfirmed"
										  FontWeight="Normal"
										  Margin="0,0,0,0"
										  Grid.Column="8"
										  Grid.Row="17"
										  FontSize="14"
										  HorizontalAlignment="Stretch"
										  VerticalAlignment="Center"
										  SelectedValue="{Binding PaidDividendScheduleConfirmed, UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}"
										  ItemsSource="{Binding YesNoOptionsList}"
										  Height="25" />

								<Border BorderBrush="Black"
										BorderThickness=".5"
										Grid.Column="10"
										Grid.Row="17"
										Background="White"
										Height="25"
										HorizontalAlignment="Stretch">
									<TextBlock Name="PaidDividendScheduleConfirmedResult"
											   Margin="0,3,0,3"
											   FontSize="12"
											   HorizontalAlignment="Center"
											   Background="White">
										<TextBlock.Style>
											<Style>
												<Style.Triggers>
													<DataTrigger Binding="{Binding PaidDividendScheduleConfirmed, UpdateSourceTrigger=PropertyChanged}"
																 Value="No">
														<Setter Property="TextElement.Foreground"
																Value="Red"></Setter>
														<Setter Property="TextBlock.Text"
																Value="UnConfirmed"></Setter>
													</DataTrigger>
													<DataTrigger Binding="{Binding PaidDividendScheduleConfirmed, UpdateSourceTrigger=PropertyChanged}"
																 Value="Yes">
														<Setter Property="TextElement.Foreground"
																Value="Green"></Setter>
														<Setter Property="TextBlock.Text"
																Value="Confirmed"></Setter>
													</DataTrigger>
												</Style.Triggers>
											</Style>
										</TextBlock.Style>
